

  <div class="media">
    <div class="media-left">
      <a href="#" class="img-link">
        <% if object.thumb_image_url.nil? %>
          <img class="media-object" data-src="holder.js/64x64" alt="64x64" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 64px; height: 64px;">
        <% else %> 
          <%= image_tag object.thumb_image_url, :class => "media-object user-pic", :style => "width: 64px; height: 64px;" %>

          <span class="hidden"><%= object.medium_image_url %></span>
        <% end %>
        
      </a>
    </div>
    <div class="media-body">
      <h4 class="media-heading" id="nested-media-heading"><%= object.title %><a class="anchorjs-link" href="#nested-media-heading"><span class="anchorjs-icon"></span></a></h4>
      Jumlah: <%= object.amount  %>
      <br /> 
      <%= object.description %>
      <br />


      <%= link_to edit_reimburse_reimburse_detail_url(parent, object), 
              :class => "btn  btn-xs" , 
              :role => "button" do %>
          <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>  
          Edit
      <% end %>  


<!--       <button type="button"  
        class="btn btn-xs" >
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
      </button> -->
      <button type="button"  
        class="btn btn-xs btn-danger" >
        <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
        Delete
      </button>
    </div>
    <div class="media-right ">

   
    </div>
  </div> 